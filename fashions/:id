<script>
    $(document).ready(function(e){
        init(e);
        loadMallDataCached(renderPage);
    })
    
    function renderPage(){
        var gallery = getFashionBySlug('ptc_both--2');
        if (gallery != undefined){
            var gallery_images = gallery.fashion_images
            renderGallery("#gallery_container", "#gallery_template", gallery_images);
        }
        show_content();
    }
</script>